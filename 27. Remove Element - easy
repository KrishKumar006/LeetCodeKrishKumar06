class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int k = 0;
        int z = nums.size();
        z = z-1;
        
        
        for (int i =0; i < nums.size(); i++)
        {
            if(nums[i] != val)
            {
                k++;
            }
        }
        for (int i = 0; i < k; i++)
        {   
            while (nums[i] == val)
            {
                while (nums[z] == val)
                {
                    z--;
                }
                swap(nums[i],nums[z]);
            }
            
        }
        return k;
    }
};
